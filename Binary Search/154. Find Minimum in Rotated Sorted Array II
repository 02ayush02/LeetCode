class Solution {
public:
    int findMin(vector<int>& nums) {
        int n = nums.size();
        int low = 0, high = n - 1;

        if (nums[low] < nums[high]) return nums[0];

        while(low < high) {
            int mid = low + ((high - low) >> 1);

            if (nums[mid] > nums[high]) {
                low = mid + 1;
            } else if (nums[mid] < nums[low]) {
                high = mid;
                low++;
            } else {
                high--;
            }
        }

        return nums[high];
    }
};
